  "devDependencies": {
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "jest-environment-jsdom-11.0.0": "^20.0.9",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
